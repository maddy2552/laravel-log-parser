{
    "name": "maddy2552/laravel-log-parser",
    "description": "Laravel Log Parser provides an interface to parse and group laravel logs for sending them to email.",
    "keywords": ["laravel", "log", "parse", "email"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/maddy2552/laravel-log-parser/issues",
        "source": "https://github.com/maddy2552/laravel-log-parser"
    },
    "authors": [
        {
            "name": "Kirill Kushnaryov",
            "email": "kushnarev.k25@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "illuminate/support": "^8.0",
        "illuminate/console": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Maddy2552\\LaravelLogParser\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Maddy2552\\LaravelLogParser\\LaravelLogParserServiceProvider"
            ]
        }
    }
}
